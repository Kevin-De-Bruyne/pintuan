/* components/goodsList.wxss */
/* 商品详情 */
.goods_detail{
  background-color: #fff;
  padding: 10rpx 3% 10rpx 3%;
  border-radius: 15rpx;
  position: relative;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
}
.goods_details{
  /* background-color: #fff; */
  padding: 10rpx 3% 10rpx 3%;
  border-radius: 15rpx;
  position: relative;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
}
.goods_left{
  height: 220rpx;
  width: 220rpx;
  border-radius: 10rpx;

}
.goods_left image{
  width: 100%;
  border-radius: 10rpx;
  height: 100%;
}
.goods_right{
  display: flex;
  flex-direction: column;
  padding-left: 30rpx;
  width: 60%;
}
.goods_r_t{
  font-size: 25rpx;
  font-weight: bold;
}
.goods_r_c{
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.now_price{
  color: rgb(251,81,46);
  font-size: 28rpx;
  font-weight: bold;
}
.old_price{
  color: rgb(175,175,175);
  padding-left: 10rpx;
  font-size: 23rpx;
  text-decoration: line-through;
}
.goods_r_c_r{
  display: flex;
  flex-direction: row;
}
.user_list{
  margin-left: -5rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}

.user_list image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.goods_r_b{
  width: 100%;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
}
.goods_r_b_l{
  width: 70%;
  height: 70rpx;
  background-image: linear-gradient(to right,orange,red);
  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
}
.goods_r_b_l_t{
  padding-left: 20rpx;
  padding-top: 10rpx;
  color: white;
  font-size: 19rpx;
}
.goods_r_b_l_b{
  padding-left: 20rpx;
  color: white;
  font-size: 19rpx;

}
.goods_r_b_r{
  width: 30%;
  background-color: rgb(255,228,94);
  height: 70rpx;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
