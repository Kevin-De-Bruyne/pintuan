/* pages/account_list/account_list.wxss */
.account{
  margin: 0;
  padding:0;
}
.content{
  font-size: 26rpx;
}
.account_list{
  display: flex;
  padding: 10rpx 20rpx;
  flex-direction: column;
  background-color: #fff;
  border-bottom: 2px solid rgb(238,238,238);
  padding-top: 30rpx;
}
.account_money{
  margin-top: 30rpx;
}
.btn{
  background-color: rgb(252,86,47);
  margin-top: 40rpx;
  text-align: center;
  color: white;
  border-radius: 40rpx;
  padding: 20rpx 100rpx;
  margin-bottom: 30rpx;
}
.bottom{
  display: flex;
  justify-content: center;
}
.van-dialog__message{
  text-align: left!important;
}